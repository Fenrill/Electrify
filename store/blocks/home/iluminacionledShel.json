{
    "flex-layout.row#iluminacionled":{
      "props":{
        "blockClass":"iluminacionledShelf"
      },
      "children":[
        "flex-layout.col#iluminacionled"
      ]
    },
    "flex-layout.col#iluminacionled":{
      "props":{"blockClass":"iluminacionledColShelf"},
      "children":[
        "list-context.product-list#iluminacionled"
      ]
    },
    "rich-text#iluminacionled":{
      "props": {
        "text": "## ILUMINACIÃ“N LED",
        "blockClass": "iluminacionledTitle"
      }
    },
