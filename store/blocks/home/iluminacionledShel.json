      },
      "children":[
        "product-selling-price#iluminacionledsummaryMain",
        "product-list-price",
        "product-price-savings#iluminacionledsummaryMain"
        
      ]
    },
    "flex-layout.row#selling-price-main":{
      "props": {
        "colGap": 2,
        "preserveLayoutOnMobile": true,
        "preventHorizontalStretch": true,
        "marginBottom": 4
      },
      "children": [
      ]
    },
    "vtex.store-components:product-brand#iluminacionled": {
      "props": {
        "displayMode": "text",
        "withLink":"text"
      }
    },
    "add-to-cart-button#iluminacionled":{
      "props":{
        "text":"Agregar al Carrito"
      }
    },
  
    "slider-layout#iluminacionled": {
      "props": {
        "itemsPerPage": {
