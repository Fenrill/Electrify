        "fullWidth": false,
        "blockClass": "shelf"
      }
    },
    "product-selling-price#iluminacionledsummaryMain":{
      "props":{
        "blockClass": "priceSummaryMain"
      }
    },
    "product-price-savings#iluminacionledsummaryMain": {
      "props": {
        "markers": [
          "discount"
        ],
        "blockClass": "summaryMainIluminacionled"
      }
    }
